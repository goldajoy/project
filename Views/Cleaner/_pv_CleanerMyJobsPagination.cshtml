@model ClassLibrary.WebModels.CleanerMyJobsModelPagenation
@{
    var IndexModel = new ClassLibrary.WebModels.CleanerMyJobsModelPagenation();
    IndexModel.JobStatus = Model.JobStatus;
    IndexModel.nextIndex = Model.nextIndex;
    IndexModel.cleanerId = Model.cleanerId;
    IndexModel.currentIndex = Model.currentIndex;
    
    var jobListCountIndex = new EntitiesLibrary.Data.WebService().cleanerListMyJobsCount(IndexModel);
}

@if (IndexModel.currentIndex > 0)
{
    <li><a class="a-cleaner-myjob-pageChange-prev" style="cursor:pointer;">Prev</a></li>
}

@if (jobListCountIndex > 0)
{
    <li><a class="a-cleaner-myjob-pageChange-next" style="cursor:pointer;">Next</a></li>
}