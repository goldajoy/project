                        <span id="spn-Address" style="color:red;display:none;"></span>
                    </div>

                    <div class="form-group">
                        @Html.TextBoxFor(m => m.Property.Suburb, new { autocomplete = "off", placeholder = "Suburb", Name = "Suburb", @class = "form-control" })
